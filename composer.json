{
    "name": "fortsq/helpers",
    "description": "Functions set for private using",
    "keywords": [
        "helpers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry V. Romanov",
            "email": "fortsq@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}